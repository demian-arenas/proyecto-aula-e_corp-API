{
  "name": "e-corp-db",
  "version": "1.0.0",
  "description": "E-Corp REST API Proyecto Aula",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && NODE_ENV=test ava --require babel-register test/**-test.js",
    "home": "micro -p 5000 home.js",
    "user": "micro -p 5001 user.js",
    "admin": "micro -p 5002 admin.js",
    "game": "micro -p 5003 game.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/demian-arenas/proyecto-aula-e_corp-API.git"
  },
  "author": "Demian Arenas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/demian-arenas/proyecto-aula-e_corp-API/issues"
  },
  "homepage": "https://github.com/demian-arenas/proyecto-aula-e_corp-API#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-eslint": "^6.0.5",
    "babel-register": "^6.9.0",
    "request-promise": "^3.0.0",
    "standard": "^7.1.2",
    "test-listen": "^1.0.0"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "gravatar": "^1.5.2",
    "http-hash": "^2.0.0",
    "jsonwebtoken": "^7.0.1",
    "micro": "^4.1.1",
    "token-extractor": "^0.1.6",
    "uuid-base62": "^0.1.0"
  }
}
